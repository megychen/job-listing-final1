<%= simple_form_for @job do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :wage_lower_bound, :label => "Lower Salary" %>
  <%= f.input :wage_upper_bound, :label => "Upper Salary" %>
  <%= f.input :contact_email %>
  <%= f.input :is_hidden %>
  <%= f.submit "Submit", class: "btn btn-primary" %>
<% end %>
